<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbos.app.sys.mapper.DictTypeMapper">
    <!--得到字典类型数据-->
    <select id="getDictTypeList" parameterType="java.util.Map" resultType="com.jbos.app.sys.pojo.DictType">
        SELECT TYPEID,TYPENAME
        FROM JBOS_DICT_TYPE
        ORDER BY TYPEID
    </select>

</mapper>